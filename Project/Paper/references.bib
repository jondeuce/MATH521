	issn = {0039-2499, 1524-4628},
	url = {http://stroke.ahajournals.org/content/37/5/1211},
	doi = {10.1161/01.STR.0000217258.63925.6b},
	abstract = {Background and Purpose— Although gray matter (GM) and white matter (WM) have differing neurochemical responses to ischemia in animal models, it is unclear whether this translates into differing thresholds for infarction. We studied this issue in ischemic stroke patients using magnetic resonance (MR) techniques.
Methods— MR studies were performed in patients with acute hemispheric ischemic stroke occurring within 24 hours and at 3 months. Cerebral blood flow (CBF), cerebral blood volume (CBV), mean transit time (MTT), and apparent diffusion coefficient (ADC) were calculated. After segmentation based on a probabilistic map of GM and WM, tissue-specific diffusion and perfusion thresholds for infarction were established.
Results— Twenty-one patients were studied. Infarction thresholds for CBF were significantly higher in GM (median 34.6 mL/100 g per minute, interquartile range 26.0 to 38.8) than in WM (20.8 mL/100 g per minute; interquartile range 18.0 to 25.9; P{\textless}0.0001). Thresholds were also significantly higher in GM than WM for CBV (GM: 1.67 mL/100 g; interquartile range 1.39 to 2.17; WM: 1.19 mL/100 g; interquartile range 0.94 to 1.53; P{\textless}0.0001), ADC (GM: 918×10−6 mm2/s; 868 to 975×10−6; WM: 805×10−6; 747 to 870×10−6; P{\textless}0.001), and there was a trend toward a shorter MTT in GM (GM 4.94 s, 4.44 to 5.38; WM 5.15, 4.11 to 5.68; P=0.11).
Conclusions— GM has a higher infarction threshold for CBF, CBV, and ADC than WM in patients within 24 hours of ischemic stroke onset. Hence, when assessing patients for potential therapies, tissue-specific rather than whole-brain thresholds may be a more precise measure of predicting the likelihood of infarction.},
	language = {en},
	number = {5},
	urldate = {2017-03-31},
	journal = {Stroke},
	author = {Arakawa, Shuji and Wright, Peter M. and Koga, Masatoshi and Phan, Thanh G. and Reutens, David C. and Lim, Indra and Gunawan, Marveyles R. and Ma, Henry and Perera, Nilupul and Ly, John and Zavala, Jorge and Fitt, Gregory and Donnan, Geoffery A.},
	month = may,
	year = {2006},
	pmid = {16574931},
	keywords = {magnetic resonance imaging, diffusion-weighted, magnetic resonance imaging, perfusion-weighted, penumbra, stroke, acute, white matter},
	pages = {1211--1216},
	file = {Full Text PDF:/home/jon/.mozilla/firefox/ryxvnmbq.default/zotero/storage/SM4PZA2I/Arakawa et al. - 2006 - Ischemic Thresholds for Gray and White Matter.pdf:application/pdf;Snapshot:/home/jon/.mozilla/firefox/ryxvnmbq.default/zotero/storage/WFUHEXJU/1211.html:text/html}
}

@article{deistung_susceptibility_2008,
