	keywords = {blood, BOLD, extravascular, intravascular, oxygenation, transverse relaxation},
	pages = {592--597},
	file = {Full Text PDF:/home/jon/.mozilla/firefox/ryxvnmbq.default/zotero/storage/6TZBJCUB/Zhao et al. - 2007 - Oxygenation and hematocrit dependence of transvers.pdf:application/pdf;Snapshot:/home/jon/.mozilla/firefox/ryxvnmbq.default/zotero/storage/FM8VNJMV/abstract.html:text/html}
}

